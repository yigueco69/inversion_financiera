<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>		
		<record id="seq_numero_cuenta" model="ir.sequence"> 
		    <field name="name">Numero de tarea</field> 
		    <field name="code">seq_numero_cuenta</field> 
		    <field name="prefix">%(year)s%(month)s</field> 
		    <field name="padding">7</field> 
		</record>

		<record id="seq_numero_transaccion" model="ir.sequence"> 
		    <field name="name">Numero de transaccion</field> 
		    <field name="code">seq_numero_transaccion</field> 
		    <field name="prefix">t_%(year)s%(month)s</field> 
		    <field name="padding">7</field> 
		</record>
		<record id="seq_numero_inversion" model="ir.sequence"> 
		    <field name="name">Numero de inversion</field> 
		    <field name="code">seq_numero_inversion</field> 
		    <field name="prefix">inv_%(year)s%(month)s</field> 
		    <field name="padding">7</field> 
		</record>


	</data>
</openerp>